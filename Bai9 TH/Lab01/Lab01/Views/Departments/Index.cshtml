@model IEnumerable<Lab01.Models.Department>

@{
    ViewBag.Title = "Index";
}

<h2>Danh sách</h2>

<p>
    @Html.ActionLink("Thêm mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.deptname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.deptname)
        </td>
        <td>
            @Html.ActionLink("Chỉnh sửa", "Edit", new { id=item.deptid }) |
            @Html.ActionLink("Chi tiết", "Details", new { id=item.deptid }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.deptid })
        </td>
    </tr>
}

</table>
