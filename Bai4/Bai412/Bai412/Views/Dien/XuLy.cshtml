
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <h3>Hóa đơn tiền điện</h3>
    <table border="1">
        <tr>
            <td>Chủ hộ: </td>
            <td>@ViewBag.ten</td>
        </tr>
        <tr>
            <td>Số điện tiêu thụ: </td>
            <td>@ViewBag.soDien</td>
        </tr>
        <tr>
            <td>Tiền phải trả: </td>
            <td>@ViewBag.tien</td>
        </tr>
    </table>
</body>
</html>

